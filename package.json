{
  "name": "weather-app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "detox.build.ios": "detox build --configuration ios.sim.debug",
    "detox.test.ios": "detox test --configuration ios.sim.debug",
    "detox.build.android": "detox build --configuration android.emu.debug",
    "detox.test.android": "detox test --configuration android.emu.debug",
    "detox.build.android.device": "detox build --configuration android.att.debug",
    "detox.test.android.device": "detox test --configuration android.att.debug",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx"
  },
  "dependencies": {
    "@react-navigation/native": "^6.1.1",
    "@react-navigation/native-stack": "^6.9.7",
    "isomorphic-fetch": "^3.0.0",
    "react": "18.1.0",
    "react-native": "0.70.6",
    "react-native-safe-area-context": "4.3.1",
    "react-native-screens": "~3.15.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@callstack/eslint-config": "^13.0.2",
    "@testing-library/jest-native": "^5.4.1",
    "@testing-library/react-native": "^11.5.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/isomorphic-fetch": "^0.0.36",
    "@types/jest": "^26.0.23",
    "@types/react": "^18.0.21",
    "@types/react-native": "^0.70.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^26.6.3",
    "detox": "^20.1.1",
    "eslint": "^8.31.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "jest": "^29",
    "metro-react-native-babel-preset": "0.72.3",
    "pod-install": "^0.1.38",
    "react-test-renderer": "18.1.0",
    "typescript": "^4.8.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/e2e/"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js"
    }
  }
}
